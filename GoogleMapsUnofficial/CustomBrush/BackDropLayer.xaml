<UserControl
    x:Class="GoogleMapsUnofficial.CustomBrush.BackDropLayer"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:GoogleMapsUnofficial.CustomBrush"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:fluent="using:GoogleMapsUnofficial.CustomBrush"
    mc:Ignorable="d"
    d:DesignHeight="420"
    d:DesignWidth="720">

    
        <Grid x:Name="rootGrid" Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
            <local:TiledBackground
                               BackgroundImage="ms-appx:///Assets/InAppIcons/NoiseAsset_256X256_PNG.png"
                               Width="{Binding ActualWidth, ElementName=rootGrid}" Height="{Binding ActualHeight, ElementName=rootGrid}"/>
        
    </Grid>
    
</UserControl>
